<h3>Update Player</h3>
    <form id="updatePlayer" action="/Player/UpdatePlayer/" method="POST">
        ID Number: <input type="text" name="ID_Update"><br>
        First Name: <input type="text" name="New_First_Name"> <br>
        Last Name: <input type="text" name="New_Last_Name"> <br>
        College: <input type="text" name="New_College"> <br>
        Number: <input type="number" name="New_Number"> <br>
        Team: <select name="New_Team">
            {{#each Team}}
            <option value="{{Abbreviation}}"> {{Abbreviation}} </option>
            {{/each}} </select> <br>
        <input type="submit" value="Submit">
    </form>

